{
    "name": "cdowdy/tinypng",
    "description": "A Bolt Extension Using TinyPNG to optimize your images",
    "type": "bolt-extension",
    "keywords": [
        "image optimization",
        "tinypng",
        "PNG optimizer",
        "JPG optimizer",
        "web performance"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "tinify/tinify": "^1.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Cory Dowdy",
            "email": "cory@corydowdy.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\cdowdy\\tinypng\\": [ "", "src/" ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\cdowdy\\tinypng\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\cdowdy\\tinypng\\TinyPNGExtension",
        "bolt-icon": "tinypng-icon.png",
        "bolt-screenshots": [
            "screenshots/tinypng-dashboard.png",
            "screenshots/tinypng-upload.png",
            "screenshots/tinypng-dropdown_0323017.png",
            "screenshots/tinypng-directories.png",
            "screenshots/tinypng-directory-search.gif",
            "screenshots/tinypng-rename.png",
            "screenshots/tinypng-spinner.png"
        ]
    }
}
